{
  "_info": {
    "ruleset": "default",
    "version": "1.0.0",
    "date": "2018-08-15",
    "org": "Mapbox",
    "description": "Minimal rules that only match high-confidence patterns"
  },
  "special": {
    "FileSizeRestrictionInMB": 50.0,
    "NumberOfLinesRestriction": 50000,
    "GitHookNumberOfChangesRestriction": 50000,
    "CFTemplateSecureParameters": [
      "_ACCESS_KEY(_ID)?$",
      "^KMS_(KEY|REGION)$",
      "^(NPM|GITHUB|MAPBOX)(_ACCESS)?_TOKEN$",
      "^(Npm|NPM|Git[Hh]ub|Mapbox)(Access)?Token$",
      "^(StripeSecret|STRIPE_TOKEN)$",
      "^SUMO_(EMAIL|PASS)$",
      "^PYPI_PASSWORD$",
      "^(AWS_ACCESS_KEY_ID|AWS_SECRET_ACCESS_KEY)$",
      "^(SSHKey|SSH_KEY|SshKey|SSHkey)$",
      "Mapbox.*AccessToken",
      ".*Pager[dD]utyToken"
    ],
    "IgnoreFiles": [
